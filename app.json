{
  "expo": {
    "name": "ezekiel-ekunola",
    "slug": "ezekiel-ekunola",
    "description": "Mobile Portfolio for Ezekiel Ekunola",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
